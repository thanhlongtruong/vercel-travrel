{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/getUser",
      "dest": "/routers/Account.js"
    },
    {
      "src": "/api/.*",
      "dest": "/routers/RouterDH.js"
    },
    {
      "src": "/api/.*",
      "dest": "/routers/RouterFlight.js"
    },
    {
      "src": "/api/.*",
      "dest": "/routers/RouterTicket.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
